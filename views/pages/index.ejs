                    <a class="grid-item" href="/projects/apache_pest_control"><img src="" alt="Apache Junction Pest Control"></a>
                    <a class="grid-item" href="/projects/gresham_fencing"><img src="" alt="Gresham Fencing"></a>

                </div>
            </div>
            <div id="contact">
                <h2>Contact Me</h2>
                <p>* All fields are required</p>
                <form>
                    <input type="text" id="name" name="name" placeholder="Name" required/>
                    <input type="email" id="email" name="email" placeholder="Email" required/>
                    <textarea name="message" rows="10" cols="30" placeholder="Put message here"></textarea>
                    <input type="submit" value="Submit"/>
                </form>
            </div>
        </main>
        <footer>
            <% include ../partials/footer.ejs %>
        </footer>

        <script src="hamburger.js"></script>
